{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sthaleWCUSTest"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\sthale01.redmond.corp.microsoft.com\\c$"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "redmond\\sthale"
		},
		"CSVBlobCreated_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2f33c621-0d70-417c-a0a4-2ebd053647fe/resourceGroups/AzureDnsPpe/providers/Microsoft.Storage/storageAccounts/sthaleparttest"
		},
		"XLSBlobCreated_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2f33c621-0d70-417c-a0a4-2ebd053647fe/resourceGroups/AzureDnsPpe/providers/Microsoft.Storage/storageAccounts/sthaleparttest"
		},
		"XLSXBlobCreated_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2f33c621-0d70-417c-a0a4-2ebd053647fe/resourceGroups/AzureDnsPpe/providers/Microsoft.Storage/storageAccounts/sthaleparttest"
		},
		"trigger1_properties_pipeline1_parameters_myName": {
			"type": "string",
			"defaultValue": "abc"
		},
		"trigger1_properties_pipeline1_parameters_pwd": {
			"type": "string",
			"defaultValue": "**********"
		},
		"trigger2_properties_pipeline1_parameters_myName": {
			"type": "string",
			"defaultValue": "abc"
		},
		"trigger2_properties_pipeline1_parameters_pwd": {
			"type": "string",
			"defaultValue": "**********"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sthaleWCUSTest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"CSVBlobCreated_properties_typeProperties_scope": {
						"value": "[parameters('CSVBlobCreated_properties_typeProperties_scope')]"
					},
					"XLSBlobCreated_properties_typeProperties_scope": {
						"value": "[parameters('XLSBlobCreated_properties_typeProperties_scope')]"
					},
					"XLSXBlobCreated_properties_typeProperties_scope": {
						"value": "[parameters('XLSXBlobCreated_properties_typeProperties_scope')]"
					},
					"trigger1_properties_pipeline1_parameters_myName": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_myName')]"
					},
					"trigger1_properties_pipeline1_parameters_pwd": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_pwd')]"
					},
					"trigger2_properties_pipeline1_parameters_myName": {
						"value": "[parameters('trigger2_properties_pipeline1_parameters_myName')]"
					},
					"trigger2_properties_pipeline1_parameters_pwd": {
						"value": "[parameters('trigger2_properties_pipeline1_parameters_pwd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
